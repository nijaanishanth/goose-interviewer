# Copy this file to .env and fill in your secrets
# Daily API (recommended for working room creation now)
DAILY_API_KEY=cea97b723b52b7e8631e81ae27b09e9049b1220bdcf6ebb68f2bf63a2c0dc6ce
# Optional: restrict to your domain or pre-create rooms in the Daily dashboard
# DAILY_SUBDOMAIN=your-subdomain # e.g. myteam (for URLs like https://myteam.daily.co/<room>)

# Optional: Tavus integration (server-side only)
TAVUS_API_KEY=
# Base URL and endpoint path for Tavus conversations (adjust to Tavus docs)
TAVUS_API_BASE=https://api.tavus.ai
TAVUS_CONVERSATION_PATH=/v1/conversations
# Your replica identifier (or model id) used to start a conversation
TAVUS_REPLICA_ID=

# CORS (comma-separated origins). Frontend default Vite URL.
ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
